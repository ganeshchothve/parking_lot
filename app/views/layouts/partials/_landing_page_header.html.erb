                <a data-event-category="Header" data-event-action="Click" data-event-name="Register Now" class="enquire-now-btn green-btn register-btn"><%=  I18n.t("devise.keys.register_now") %></a>
              </div>
              <% end %>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>
    </div>
</header>
