<header class="col-app-12 fixed left top shrink bg-white">
    <div class="container">
        <div class="pull-left left-header nav-links">
          <!--<a href="/" rel="home" class="brand-logo"><img src="<%= get_client_from_domain.logo.url || 'launchpad-logo.png' %>" data-event-category="Header" data-event-action="Click" data-event-name="Logo" /></a>-->
        </div>
        <div class="pull-right right-header">
            <div class="pull-right upper-right-header hidden-xs hidden-portrait">
              <div class="pull-right ml-2">
                <%= link_to  I18n.t("devise.keys.sign_in"), new_user_session_path, {class: 'green-btn sign-in-button', 'data-event-category': 'Header', 'data-event-action': 'Click', 'data-event-name': 'Login'} %>
