<%= javascript_tag do %>

var window_ht = $(window).height();
var window_wt = $(window).width();
var header_ht = $('header').height();

$(document).ready(function () {
    if (window_wt <= 768) {
        $(".brand-logo img").attr("src", "<%= current_client.logo.url || asset_path 'logo.svg' %>");
    }
});

<% end %>
