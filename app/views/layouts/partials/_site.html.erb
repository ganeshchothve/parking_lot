var aArray = [];
var wn_width = $(window).width();
var wn_height = $(window).height();
var wind_height = $('.header').outerHeight();

$(document).ready(function () {
  $(window).on("scroll", function() {
    if(wn_width > 992){
      if ($(window).scrollTop() > 150) {
          $('.home-header.shrink').addClass('sticky-header');
          $('.home-header .brand-logo img').attr('src','<%= current_client.logo.url || asset_path 'beyondwalls-logo.svg' %>');
      } else {
          $('.home-header.shrink').removeClass('sticky-header');
          $('.home-header .brand-logo img').attr('src','<%= current_client.logo.url || asset_path 'beyondwalls-logo-white.svg' %>')
      }
    }
  });
});
