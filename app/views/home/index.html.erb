<section class="broker-sec">
  <div class="container">
    <div class="broker-wrapper">
      <div class="broker-left text-right">
        <h2>BROKER</h2>
        <ul class="broker-left-list">
          <li>NO Brokerage</li>
          <li>No information about new launches </li>
          <li>No tie-ups with TOP developers </li>
          <li>Low Quality Lead</li>
          <li>No Promotion Material of projects</li>
        </ul>
      </div>
      <div class="broker-img">
        <img src="<%= asset_path 'broker-img.png' %>">
      </div>
      <div class="broker-right">
        <h2>SUPERBROKER</h2>
        <ul class="broker-right-list text-left">
          <li>FULL Brokerage on time</li>
          <li>Get ALL information about new launches</li>
          <li>Work with TOP Developers</li>
          <li>Best quality Leads at Lowest Prices</li>
          <li>ALL Promotion Materials for every project</li>
        </ul>
      </div>
    </div>
    <div class="col-12">
      <p class="text-center cbk f500">Pune's residential market opportunity</p>
      <div class="join-broker-network-btn-wrapper text-center">
        <a href="<%= new_channel_partner_path %>" class="green-btn register-btn" data-event-category="Section"
          data-event-action="Click" data-event-name="Register on Launchpad">Register Now</a>
      </div>
    </div>
    <div class="col-12">
      <div class="stats-brokers">
        <ul>
          <li>
            <div class="stats-inner">
              <img src="<%= asset_path 'lined-up.svg' %>">
              <p>3000+<span>Lined Up Launches</span></p>
            </div>
          </li>
          <li>
            <div class="stats-inner">
              <img src="<%= asset_path 'paid-out.svg' %>">
              <p>Rs. 200 Cr.<span>Brokerage to be paid out <br>within the next 6 months</span></p>
            </div>
          </li>
        </ul>
      </div>
    </div>
  </div>
</section>
<section class="existingnew-projects-section section-padding" id="existingnew-projects">
  <div class="container">
    <h2 class="section-heading text-center cgreen">Chance to work with Pune's hottest selling properties</h2>
    <div class="row relative">
      <div class="project-listing-slider">
        <% Project.where(hot: true).limit(15).each do |project| %>
        <div class="property-item col-app-4 col-app-sm-6">
          <div class="property-item-inner">
            <div class="property-item-image">
              <%#= image_tag(project.cover_photo&.url || (asset_path 'default-project.jpg'), class: 'img-full lazy') %>
              <div class="propery-thumb-img position-relative"
                style="background-image: url('<%= project.cover_photo&.url || (asset_path 'default-project.jpg') %>')">
                <% if project.hot?%>
                <span class="hot-sellig-tag"><i class="fa fa-fire fill-danger me-1"></i>Hot Selling</span>
                <% end %>
              </div>
            </div>
            <div class="property-item-details">
              <div class="property-name cgreen f600"><%= project.name %></div>
              <p class="property-location">
                <%= "#{project.micro_market.present? ? project.micro_market&.titleize + ', ': ''}#{project.city&.titleize}".presence || '-' %>
              </p>
              <div class="property-configuration">
                <%= project.configurations.first(2)&.to_sentence(two_words_connector: ' & ') %>
                <%= project.price_starting_from.present? ? "from #{number_to_inr(project.price_starting_from)}" : '' %>
              </div>
            </div>
          </div>
        </div>
        <% end %>
      </div>
      <div id="prjt-list-prev" class=""></div>
      <div id="prjt-list-next" class=""></div>
    </div>
  </div>
</section>
