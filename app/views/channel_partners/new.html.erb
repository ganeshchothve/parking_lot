<h2 class="font-weight-bold text-success"><%= global_labels[:channel_partner] %></h2>
<p class="pb-3 text-muted">Your login will be created & sent to you once the <%= global_labels[:channel_partner] %> Team confirms your registration.</p>
<%= form_with(model: @channel_partner, local: true, html: {id: 'channel_partner_form', class: 'validate-form', multipart: true}) do |form| %>
  <% if @channel_partner.errors.full_messages.present? %>
  <div class="mb-3 alert alert-danger">
    <strong>The form contains <%= @channel_partner.errors.full_messages.length %> errors.</strong>
    <ul class="mt-3 pl-3">
    <% @channel_partner.errors.full_messages.each do |x| %>
      <li><%= x %></li>
    <% end %>
    </ul>
  </div>
  <% end %>
  <%= render "form_fields", {form: form} %>
  <div class="form-group">
    <%= form.submit "Register", class: "btn btn-primary" %>
  </div>
  <%= render "devise/shared/links" %>
<% end %>
<%= javascript_tag do %>
  $(document).ready(function(){
    FormInitializer.init($("#channel_partner_form"));
  });
<% end %>
