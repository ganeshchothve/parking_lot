<section class="projects-list device-pd">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 my-4 no-pd">
        <div class="box-card">
          <div class="box-header br-rd-tr-4 row pb-3">
            <a data-toggle="collapse" href="#my-bookings" role="button" aria-expanded="false" aria-controls="dashboard_users" class='font-weight-bold text-decoration-none col-12 p-0'>
              <h2 class="text-dark icon-set icon-home-black">
                <%= t('controller.buyer.dashboard.my_bookings') %>
              </h2>
            </a>
          </div>
          <% current_user.active_bookings.each do |booking| %>
            <div class="box-content collapse show br-rd-bl-4 bg-white p-0" id="my-bookings">
              <%= render "booking_details/details", booking_detail: booking, with_actions: true %>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</section>
