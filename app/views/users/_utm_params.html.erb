<section class="bookings my-4 mb-4 d-print-none">
  <div class="container">
    <div class="box-card">
      <div class="box-header bg-gradient br-rd-tr-4 row">
        <div class="col-12"><h2><%= t('mongoid.attributes.user.utm_params') %></h2></div>
      </div>
      <div class="box-content br-rd-bl-4 bg-white row">
        <div class="col-md-12 col-lg-12 col-sm-12">
          <ul class="booking-apart-info clear">
            <% user.utm_params.each do |key, value| %>
              <li>
                <span> <%= key %> : </span>
                <p><%= value %></p>
              </li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
