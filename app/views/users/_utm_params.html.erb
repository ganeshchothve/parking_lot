<div class="box-card d-print-none">
  <div class="box-header bg-gradient br-rd-tr-4"><h2> <%= t('mongoid.attributes.user.utm_params') %> </h2></div>
  <div class="box-content br-rd-bl-4 bg-white">
    <div class="row pd-box">
      <div class="col-md-12">
        <div class="row">
          <% user.utm_params.each do |key, value| %>
            <div class="col-md-4">
              <div class="form-group">
                <label> <%= key %> : </label>
                <div>
                  <%= value %>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
