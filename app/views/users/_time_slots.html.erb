<section class="projects-list device-pd">
  <div class="container">
    <div class="row">
      <div class="separate-line"></div>
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 no-pd">
        <div class="box-card">
          <div class="box-header br-rd-tr-4 row">
            <a data-toggle="collapse" href="#dashboard-time-slot" role="button" aria-expanded="false" aria-controls="dashboard_users" class='font-weight-bold text-decoration-none col-12 p-0'>
              <h2 class="title text-dark icon-set icon-time-black">
                <%= t('controller.buyer.dashboard.time_slots') %>
              </h2>
            </a>
          </div>
          <div class="box-content collapse show br-rd-bl-4 bg-white p-0" id="dashboard-time-slot">
            <%= render 'receipts/time_slot', {receipts: current_user.receipts, with_actions: false} %>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>