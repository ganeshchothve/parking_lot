<section class="users my-4 mb-4">
  <div class="container">
    <div class="row">
      <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd cursor-pointer" data-toggle="collapse" href="#collapseUsers" role="button" aria-expanded="false" aria-controls="collapseUsers">
        <div class="table-title pt-1">
          <h2 class="title text-dark icon-set icon-users-black">
            <%= User.model_name.human(count: users.total_entries )%>
          </h2>
        </div>
      </div>
      <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
        <div class="table-btn-set float-right pb-3">
          <button disabled="disabled" class="db-small-btn light-blue bg-white fn-400"><%= t('global.total')%> : <%= users.total_entries %></button>
        </div>
      </div>
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 no-pd bg-white collapse" id="collapseUsers">
        <%= render "admin/users/index", users: users %>
      </div>
    </div>
  </div>
</section>