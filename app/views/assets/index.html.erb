<div class="modal fade right fixed-header-footer" role="dialog" id="modal-remote-form-inner">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header bg-gradient-cd white">
        <h3 class="title"><%= params.dig(:fltrs, :document_type).try(:titleize).try(:pluralize) || Asset.model_name.human(count: 2) %></h3>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true" class="white">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <% if params.dig(:fltrs, :document_type) == 'video' %>
          <% template = ::Template::UITemplate.where(name: 'assets/_videos').first %>
          <% if template.present? %>
            <%= get_view_erb(template) %>
          <% end %>
        <% else %>
          <%= render "assets/index", assetable: @assetable %>
        <% end %>
      </div>
      <div class="modal-footer">
      </div>
    </div>
  </div>
</div>
