<section class="mb-5 mt-3">
  <div class="px-4">
    <div class="row">
    <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
      <div class="table-title pt-1">
       <!--  <h1 class="title icon-set icon-file-contract-black">
          <%= Checklist.model_name.human(count: @checklists.total_entries) %>
        </h1> -->
      </div>
    </div>
    <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
      <div class="table-btn-set float-end pb-3 text-end">
        <% if policy([:admin, Checklist]).new? %>
          <%= link_to t('helpers.new.link_name', model: Checklist.model_name.human), new_admin_checklist_path, class: 'border rounded px-3 py-2 primary border-primary' %>
          <% end %>
      </div>
    </div>
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 no-pd">
        <div class="box-card relative">
          <%= render 'index', {checklists: @checklists} %>
        </div>
      </div>
    </div>
  </div>
</section>
