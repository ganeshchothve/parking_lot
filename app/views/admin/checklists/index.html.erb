<section class="mb-5 mt-3">
  <div class="">
    <div class="row">
    <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 no-pd">
      <div class="table-btn-set float-end pb-3 text-end">
        <% if policy([:admin, Checklist]).new? %>
          <%= link_to t('helpers.new.link_name', model: Checklist.model_name.human), new_admin_checklist_path, class: 'border rounded px-3 px-3 border-primary white bg-primary rounded ms-2 modal-remote-form-link btn' %>
          <% end %>
      </div>
    </div>
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 no-pd">
        <div class="box-card relative">
          <%= render 'index', {checklists: @checklists} %>
        </div>
      </div>
    </div>
  </div>
</section>
