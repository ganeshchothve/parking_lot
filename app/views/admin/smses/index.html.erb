<section class="mb-5">
  <div class="container">
    <div class="row">
      <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
        <div class="table-title pt-1">
          <!-- <h1 class="title icon-set icon-sms-black">
            <%= Sms.model_name.human(count: @smses.total_entries ) %>
          </h1> -->
          <div class="filter-search">
            <input type="text" name="" placeholder="Search projects">
          </div>
          <div class="filter-popup">
            <a class="db-filter-btn fn-500" href="javascript:;">
              <i class="fa fa-filter fill-theme"></i>
            </a>
          </div>
        </div>
      </div>
      <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
        <div class="table-btn-set float-end pb-3 text-end mt-1">
          <% if policy([:admin, Sms]).sms_pulse? %>
            <%= link_to "SMS Pulse", sms_pulse_admin_smses_path, class: 'btn-primary fn-400 btn' %>
          <% end %>
        </div>
      </div>
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 no-pd">
        <div class="box-card relative">
          <%= render 'index', {smses: @smses} %>
          <div class="filter-box">
            <%= render 'filters', url: admin_smses_path %>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
