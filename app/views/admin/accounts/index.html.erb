<section class="my-customer my-4 mb-5 pb-5">
  <div class="container">
    <div class="row">
    <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
      <div class="table-title pt-4 pb-5">
      <img src="<%= asset_path 'user-black.svg' %>" alt="user icon"> <h1 class="title">Schemes</h1>
      </div>
    </div>
    <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
      <div class="table-btn-set float-right pt-4 pb-4">
        <a class="db-small-btn light-blue bg-white fn-400 dropdown-toggle" href="javascript:;" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Export</a>
        <div class="dropdown-menu dropdown-menu-right">
          <% Client.available_payment_gateways.collect {|x| x[:id]}.each do |acct_type| %>
            <%= link_to acct_type, new_admin_account_path(account_type: "#{acct_type}Payment".underscore), class: 'dropdown-item modal-remote-form-link' %>
          <% end %>
        </div>
      </div>
    </div>
    </div>
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 no-pd">
        <div class="box-card relative">
          <%= render 'index', {accounts: @accounts} %>
        </div>
      </div>
    </div>
  </div>
</section>
