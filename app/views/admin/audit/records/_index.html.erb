<div class="table-responsive">
  <table class='table striped-table payment-breakup' width="100%" cellpadding="10" cellspacing="0">
    <thead class="th-default">
      <tr>
        <th>Subject Class</th>
        <th>Changed By</th>
        <th>Action Type</th>
        <!-- <th>Executed Time</th> -->
        <th class="text-right">Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @audits.each do |audit| %>
      <tr>
        <td>
          <%= audit.subject_class %>
        </td>
        <td>
          <%= audit.user_name %>
        </td>
        <td>
          <%= audit.change_type %>
        </td>
        <!-- <td>
          <%#= audit.modified_at %>
        </td> -->

        <td class="text-right">
          <%= link_to 'Show', admin_audit_entry_path(audit),  {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#modal-window', class: 'btn btn-primary btn-sm'}  %>
          <div id="modal-window" class="modal hide fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="width:1250px;">
            <div class="modal-dialog" role="document">
              <div class="modal-content"></div>
            </div>
          </div>
        </td>
      </tr>
      <% end %>
      <tr>
        <td colspan="4">
          <div class="mt-5 pt-5"></div>
        </td>
      </tr>
    </tbody>
  </table>
</div>
<%= will_paginate @audits, list_classes: %w(pagination justify-content-center) %>
