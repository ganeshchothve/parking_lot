<%= form_for(@booking_detail, url: process_booking_on_project_admin_booking_details_path , remote: true, html: {class: 'validate-form modal-remote-form', "data-type": "json", id: 'project_add_booking'}) do |f| %>
<%#= f.hidden_field :lead_id, value: @booking_detail.lead_id %>
<div class="modal fade right fixed-header-footer" role="dialog" id="modal-remote-form-inner">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <%= render 'booking_on_project', f: f, project: @project %>    
    </div>
  </div>
</div>
<% end %>
<%= javascript_tag do %>
$(document).ready(function(){
  FormInitializer.init($("#project_add_booking"));
});
<% end %>
