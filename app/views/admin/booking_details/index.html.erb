<section class="mb-5">
  <div class="container">
    <div class="row">
    <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
      <div class="table-title pt-1">
        <!-- <h1 class="title icon-set icon-home-black">
          <%= BookingDetail.model_name.human(count: @booking_details.total_entries) %>
        </h1> -->
        <div class="filter-search">
          <input type="text" name="" placeholder="Search projects">
        </div>
        <div class="filter-popup">
          <a class="db-filter-btn fn-500" href="javascript:;">
            <i class="fa fa-filter fill-theme"></i>
          </a>
        </div>
      </div>
    </div>
    <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
      <div class="table-btn-set float-end pb-3">
        <% if policy([:admin, BookingDetail.new]).mis_report? %>
          <div class="new-menu display-initial  mt-2 text-end" >
            <button class="select-btn fn-400 dropdown-toggle" href="javascript:;" role="button" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><%= t('controller.users.exports.link_name', name: nil) %></button>
            <div class="dropdown-menu">
              <%= link_to t('controller.users.exports.link_name', name: 'MIS' ), mis_report_admin_booking_details_path, class: ' dropdown-item' %>
            </div>
          </div>
        <% end %>
      </div>
    </div>
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 no-pd">
        <div class="box-card relative">
          <%= render '/admin/booking_details/index', booking_details: @booking_details %>
          <div class="filter-box">
            <%= render "filters" %>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
