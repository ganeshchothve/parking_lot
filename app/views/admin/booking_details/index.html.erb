<section class="my-customer">
  <div class="container">
    <div class="row">
    <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
      <div class="table-title pt-1">
      <img src="<%= asset_path 'user-black.svg' %>" alt="user icon"> <h1 class="title">Bookings</h1>
      </div>
    </div>
    <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
      <div class="table-btn-set float-right pb-3">
        <% if policy([:admin, BookingDetail.new]).mis_report? %>
          <%= link_to "MIS report #{global_labels['units']}", mis_report_admin_booking_details_path, class: 'db-small-btn light-blue bg-white fn-400' %>
        <% end %>
        <button class="db-filter-btn white fn-500" type="button"><img src="<%= asset_path 'filter-light-blue.svg' %>" alt="filter icon"></button>
      </div>
    </div>
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 no-pd">
        <div class="box-card relative">
          <%= render '/admin/booking_details/index', booking_details: @booking_details %>
          <div class="filter-box">
            <%= render "filters" %>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
