<section class="prjt-details-sec">
  <div class="px-3 pb-5">
    <div class="row">
      <div class="col-lg-8 px-3 bg-white pt-4">
        <div class="col-12 mb-3">
          <div class="d-flex justify-content-between">
            <div>
              <h6 class="card-title d-inline fn-24 font-medium theme-txt-color me-1">Rhea Miranda</h6>
              <a href="javascript:;" class="email-icon small d-inline-block mt-1 fn-16" data-bs-toggle="popover"
                title="Email Id" data-bs-trigger="focus" data-bs-placement="bottom" data-bs-content="abc@gmail.com">
                <i class="fa fa-envelope fill-theme me-2 ms-2 fn-18"></i>
              </a>
              <a href="javascript:;" class="phone-icon small d-inline-block mt-1 fn-16" data-bs-container="body"
                data-bs-placement="bottom" data-bs-trigger="focus" data-bs-toggle="popover" title="Phone No."
                data-bs-content="+91 8521479630">
                <i class="fa fa-phone-alt fill-theme me-2 fn-18"></i>
              </a>
            </div>
            <div>
              <a href="javascript:;" class="border border-primary rounded primary p-2 fn-14 font-medium px-3">Edit
                Customer</a>
              <a href="javascript:;"
                class="bg-primary border border-primary rounded white p-2 ms-2 fn-14 font-medium px-3">Create
                Booking</a>
            </div>
          </div>
        </div>
        <div class="col-12">
          <div class="row border rounded col-12 me-0 g-0">
            <p class="fn-14 font-medium p-2 px-3 pb-0">KYC (Primary Applicant)</p>
            <div class="d-flex justify-content-between p-2 px-3 pb-0">
              <p class="fn-16 font-medium">Rhea Miranda</p>
              <div class="d-flex justify-content-end">
                <p><i class="fa fa-check-circle fill-success me-1"></i> General Info</p>
                <p class="mx-3"><span class="border-bottom border-2 wd-24 align-middle d-inline-block"></span></p>
                <p><i class="fa fa-check-circle fill-secondory me-1"></i> Bank Details</p>
                <p class="mx-3"><span class="border-bottom border-2 wd-24 align-middle d-inline-block"></span></p>
                <p><i class="fa fa-check-circle fill-secondory me-1"></i> Documents</p>
              </div>
            </div>
          </div>
        </div>


        <div class="col-12 float-start mt-3 pb-0">
          <div class="card">
            <div class="card-header theme-light-bg py-3">
              <p class="fn-12 theme-txt-color mb-0 ">Project</p>
              <h6 class="fn-20 theme-txt-color mb-0"><%= @project_unit.project_name %></h6>
            </div>
            <div class="card-body p-0 rounded-0 border-0">
              <div class="accordion theme-accordation" id="accordionExample">
                <div class="accordion-item rounded-0 border-bottom border-top-0  border-end-0 border-start-0">
                  <h2 class="accordion-header" id="headingOne">
                    <button class="accordion-button collapsed shadow-none" type="button" data-bs-toggle="collapse"
                      data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                      Requirement
                    </button>

                  </h2>
                  <div id="collapseOne" class="accordion-collapse collapse" aria-labelledby="headingOne"
                    data-bs-parent="#accordionExample">
                    <div class="accordion-body pt-0 ps-4">
                      <%= render 'dashboard/project_unit_overview', { project_unit: @project_unit } %>
                    </div>
                  </div>
                </div>
                <div class="accordion-item  border-end-0 border-start-0">
                  <h2 class="accordion-header d-flex justify-content-between" id="headingTwo">
                    <button class="accordion-button collapsed  shadow-none" type="button" data-bs-toggle="collapse"
                      data-bs-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                      Booking Details
                    </button>
                    <a href="javascript:;" class="fn-12 font-medium theme-txt-color w-25 text-end pe-3 py-3"><i
                        class="fa fa-plus fill-primary me-2 fn-12"></i>Booking</a>
                  </h2>
                  <div id="collapseTwo" class="accordion-collapse collapse" aria-labelledby="headingTwo"
                    data-bs-parent="#accordionExample">
                    <div class="accordion-body pt-0 ps-4">
                      <% if %w[superadmin admin].include?(current_user.role) && @booking_details.present? %>
                      <div class="mt-3"></div>
                      <%= render 'booking_details/show_section', booking_details: @booking_details %>
                      <% end %>
                    </div>
                  </div>
                </div>
                <div class="accordion-item border-end-0 border-start-0">
                  <h2 class="accordion-header d-flex justify-content-between" id="headingThree">
                    <button class="accordion-button collapsed shadow-none" type="button" data-bs-toggle="collapse"
                      data-bs-target="#collapseThree" aria-expanded="false" aria-controls="headingThree">
                      Documents
                    </button>

                  </h2>
                  <div id="collapseThree" class="accordion-collapse collapse" aria-labelledby="headingThree"
                    data-bs-parent="#accordionExample">
                    <div class="accordion-body pt-0 ps-4">
                      <div class="row ps-3">
                        <div class="col-12">
                          <%= render 'assets/section', record: @project_unit, assets: @project_unit.assets %>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>

              </div>
            </div>
          </div>
        </div>

        <div class="col-12 float-start mt-4">
          <h2 class="fn-16 font-medium mb-3">Internal details</h2>
          <div class="row">
            <div class="col-4">
              <p class="fn-12 font-medium mb-0 txt-light-gray">Created On</p>
              <p class="fn-14">July 3rd, 2020</p>
            </div>
            <div class="col-4">
              <p class="fn-12 font-medium mb-0 txt-light-gray">Booked on</p>
              <p class="fn-14">July 3rd, 2020</p>
            </div>
            <div class="col-4">
              <p class="fn-12 font-medium mb-0 txt-light-gray">Registration Date</p>
              <p class="fn-14">Mar 12, 2020</p>
            </div>
          </div>
        </div>
      </div>
      <div class="col-lg-4 pt-2 px-4">
        <div class="row">
          <div class="col-12 bg-white border rounded shadow pt-3 mb-3 pb-3">
            <div class="d-flex justify-content-between">
              <p class="fn-16 font-medium">History</p>
            </div>
            <div class="col-12">
              <div class="row">
                <div class="col-lg-1 dotted-line">
                  <p><span class="step-circle"></span></p>
                </div>
                <div class="col-lg-11">
                  <p class="fn-12 text-muted mb-1">July 5th, 2020</p>
                  <p class="fn-14 font-medium mb-0 ">New Booking Created</p>
                  <div class="border-bottom mb-4 pt-1"></div>
                </div>
                <div class="col-lg-1 dotted-line">
                  <p><span class="step-circle"></span></p>
                </div>
                <div class="col-lg-11">
                  <p class="fn-12 text-muted mb-1">July 5th, 2020</p>
                  <p class="fn-14 font-medium mb-0">KYC Completed</p>
                  <div class="border-bottom mb-4 pt-1"></div>
                </div>
                <div class="col-lg-1 dotted-line">
                  <p><span class="step-circle"></span></p>
                </div>
                <div class="col-lg-11">
                  <p class="fn-12 text-muted mb-1">July 4th, 2020</p>
                  <p class="fn-14 font-medium mb-0">Site Visit Conducted</p>
                  <div class="border-bottom mb-4 pt-1"></div>
                </div>
                <div class="col-lg-1 dotted-line no-dotted">
                  <p><span class="step-circle"></span></p>
                </div>
                <div class="col-lg-11">
                  <p class="fn-12 text-muted mb-1">July 4th, 2020</p>
                  <p class="fn-14 font-medium mb-0">Lead created on Launchpad</p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
