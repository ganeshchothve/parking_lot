  <table class="table my-customer-table responsive-tbl">
    <thead class="th-default">
      <tr class="bg-primary white">
        <th>Tower Name</th>
        <th>Total Units</th>
        <th>Sold / Unsold</th>
        <% ProjectUnit.distinct(:unit_configuration_name).sort.each do |uc| %>
          <th class="text-center"><%= uc %><br>(Sold / Unsold)</th>
        <% end %>
        <th>A.V (Sold)</th>
        <th>Collection</th>
      </tr>
    </thead>
    <tbody>
      <% _all_towers = @out["All Towers"] %>
      <% @out.except!("All Towers") %>
      <% @out.each do |key, value| %>
        <tr>
          <td data-title='Tower Name'><strong><%= key %></strong></td>
          <td data-title='Total Units'><%= value[:total] %></td>
          <td data-title='Sold / Unsold'><%= "#{value[:sold]}/#{value[:unsold]}" %></td>
          <% value[:configuration_wise].each do |k, v| %>
            <td class='text-center' data-title='<%= "#{k} (Sold / Unsold)" %>'><%= "#{v[:sold]}/#{v[:unsold]}" %></td>
          <% end %>
          <td data-title='A.V (Sold)'><%= number_to_indian_currency(value[:av_sold]||0) %></td>
          <td class="non-action-td" data-title='Collection'><%= number_to_indian_currency(value[:collection]||0) %></td>
        </tr>
      <% end %>
      <tr>
        <td data-title='Tower Name'><strong>All Towers</strong></td>
        <td data-title='Total Units'><strong><%= _all_towers[:total] %></strong></td>
        <td data-title='Sold / Unsold'><strong><%= "#{_all_towers[:sold]}/#{_all_towers[:unsold]}" %></strong></td>
        <% _all_towers[:configuration_wise].each do |k, v| %>
          <td class='text-center' data-title='<%= "#{k} (Sold / Unsold)" %>'><strong><%= "#{v[:sold]}/#{v[:unsold]}" %></strong></td>
        <% end %>
        <td data-title='A.V (Sold)'><strong><%= number_to_indian_currency(_all_towers[:av_sold]||0) %></strong></td>
        <td class="non-action-td" data-title='Collection'><strong><%= number_to_indian_currency(_all_towers[:collection]||0) %></strong></td>
      </tr>
    </tbody>
  </table>
