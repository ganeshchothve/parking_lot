						<tr>
							<td class="text-center">
								06
							</td>
							<td>
								FRP
							</td>
							<td>
								<%= number_to_indian_currency(@project_unit.floor_rise.round(2)) %>
							</td>
						</tr>
						<tr>
							<td class="text-center">
								07
							</td>
							<td>
								Base Price + PLC + FRP
							</td>
							<td>
								<%= @project_unit.base_rate + @project_unit.premium_location_charges + @project_unit.floor_rise %>
							</td>
						</tr>
						<tr>
							<td class="text-center">
								08
							</td>
							<td>
								Land Value
							</td>
							<td>
								<%= number_to_indian_currency(@project_unit.land_price.round(2)) %>
							</td>
						</tr>
						<tr>
							<td class="text-center">
								09
							</td>
							<td>
								Construction Value
							</td>
							<td>
								<%= number_to_indian_currency(((@project_unit.base_rate + @project_unit.premium_location_charges + @project_unit.floor_rise) - (@project_unit.land_rate))*(@project_unit.saleable).round(2)) %>
							</td>
						</tr>
						<tr>
							<td class="pink">
								&nbsp;
							</td>
							<td class="pink">
								Agreement Value
							</td>
							<td class="pink">
								<%= number_to_indian_currency(project_unit.agreement_price.round) %>
							</td>
						</tr>
					</table>
