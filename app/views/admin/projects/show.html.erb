<% inventory_avl = @project.enable_inventory %>
<% project_labels = t('mongoid.attributes.project') %>
<% interest = current_user.interested_projects.in(status: %w(subscribed approved)).where(project_id: @project.id).presence %>
<section class="prjt-details-sec">
    <div class="px-3">
        <div class="row">
            <div class="col-lg-8 px-3 prjt-dtls-wrap pt-3">
                <div class="prjt-inner-box">
                    <div class="prjt-details-header col-12 float-start bt-br">
                        <div class="col-lg-6 col-12 float-start text-start">
                            <h1><%= @project.name || '-' %></h1>
                            <p class="prjt-devlprs mb-2"><%= @project.developer_name || '-' %></p>
                            <p class="prjt-location"><i class="fa fa-map-marker-alt me-2"></i><%= @project.micro_market || '-' %></p>
                        </div>
                        <div class="col-lg-6 col-12 float-end text-end  px-0 pt-2">
                          <div class="d-block">
                            <%= link_to_if (launchbook_url = @project.assets.where(document_type: 'sales_presentation').first&.file&.url&.presence), '<i class="fa fa-lock fill-theme me-3"></i>Launchbook'.html_safe, launchbook_url, class: 'prjt-lauchbook', target:'_blank' do |name|
                              ''
                            end%>
                            <% if allow_interest_subscription?(@project) %>
                              <div class="prjt-share-save pt-3 pb-2 d-inline ms-3">
                                <!--<a href="javascript:;" class="share-location me-3"><i class="fa fa-share-alt fill-theme me-2"></i>Share location</a>-->
                                <a href="javascript:;" class="prjt-save"></a>
                                <%= link_to_unless interest.present?, '<i class="fa fa-heart fill-theme me-2"></i>Show Interest'.html_safe, admin_user_interested_projects_path(current_user, interested_project: {project_id: @project.id, event: 'subscribe'}), method: :post, class: 'prjt-save' do |name|
                                  '<i class="fa fa-heart fill-danger"></i>'.html_safe
                                end
                              %>
                              </div>
                            <% end %>
                            <% if policy([current_user_role_group, InterestedProject]).report? %>
                              <% ip_count = User.where(User.user_based_scope(current_user, params)).filter_by_interested_project(@project.id).count %>
                              <%= link_to "<i class='fa fa-heart fill-danger me-2'></i>#{ip_count}".html_safe, admin_users_path(fltrs: {interested_project: @project.id}), class: 'prjt-save' %>
                            <% end %>
                            <% if current_user.role.in?(%w(superadmin admin)) %>
                              <div class="dropdown show d-inline">
                                <a class="btn table-dropdown-btn" href="#" role="button" id="dropdownMenuLink" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-ellipsis-v" aria-hidden="true"></i></a>
                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuLink">
                                  <%= render "#{current_user_role_group}/projects/links", project: @project %>
                                </div>
                              </div>
                            <% end %>
                          </div>
                        </div>
                    </div>
                    <div class="prjt-gallery pb-3 col-12 float-start">
                        <div class="prjt-gallery-inner">
                            <ul>
                            <% @assets = @project.assets.where(document_type: "images") %>
                            <% @assets[0..5].each_with_index do |asset, index|%>
                                <li class="<%= (@assets.size > 6 && index == 5) ? "more-img" : "" %>">
                                    <a href="javascript:;" data-bs-toggle="modal" data-bs-target="#galleryModal">
                                     <div class="wd-100 ht-77 rounded" style="background-image: url(<%= asset.file.url %>);background-size:cover;background-position:center"></div>
                                     <% if (@assets.size > 6 && index == 5) %>
                                     <p>+ <%= @assets.size - 6 %></p>
                                     <% end %> 
                                     </a>
                                </li>
                            <% end %>
                            </ul>
                        </div>
                    </div>
                    <div class="prjt-desc-box col-12 float-start">
                        <div class="prjt-desc-tab  col-12 float-start">
                            <nav class="prjt-menu col-12 float-start">
                                <ul class="" id="nav-tab">
                                    <li class="">
                                        <a class="active" id="nav-overview-tab" rel="tab-overview" href="javascript:;">Overview</a>
                                    </li>
                                    <li class="">
                                        <a class="" id="nav-location-tab" rel="tab-location" href="javascript:;">Location</a>
                                    </li>
                                    <% if inventory_avl %>
                                      <li class="">
                                          <a class="" id="nav-configure-tab" href="javascript:;" rel="tab-configure">Configurations</a>
                                      </li>
                                    <% end %>
                                      <li class="">
                                          <a class="" id="nav-collaterals-tab" href="javascript:;" rel="tab-collaterals">Collaterals</a>
                                      </li>
                                    <% if @project.amenities.present? %>
                                      <li class="">
                                          <a class="" id="nav-amenities-tab" href="javascript:;" rel="tab-amenities">Amenities</a>
                                      </li>
                                    <% end %>
                                    <% if @project.specifications.present? %>
                                      <li class="">
                                          <a class="" id="nav-specifications-tab" href="javascript:;" rel="tab-specifications">Specifications</a>
                                      </li>
                                    <% end %>
                                    <% if @project.timeline_updates.present? %>
                                      <li class="">
                                          <a class="" id="nav-timeline-tab" href="javascript:;" rel="tab-timeline">Project Timeline</a>
                                      </li>
                                    <% end %>
                                </ul>
                            </nav>
                        </div>
                        <div class="prjt-desc-tab-wrap col-12 float-start">
                            <div class="prjt-desc-tab col-12 bt-br float-start" id="tab-overview">
                                <div class="tab-header">
                                    <h2 class="tab-title">Overview</h2>
                                </div>
                                <div class="tab-body pt-2">
                                    <div class="col-lg-5 col-12 overview-left float-start">
                                        <p>
                                            <span>Configuration</span>
                                            <% flats_text = @project.project_type.include?("residential") ? " Flats" : "" %>
                                            <%= inventory_avl ? DashboardDataProvider.configurations(@project).to_sentence + flats_text : @project.configurations&.to_sentence + flats_text %> <%= inventory_avl ? "| #{@project.project_units.min(:saleable)&.round} - #{@project.project_units.max(:saleable)&.round} #{@project.area_unit}" : '' %>
                                        </p>
                                        <p>
                                            <span>Property Value</span>
                                            <%= number_to_inr(@project.price_starting_from) %> - <%= number_to_inr(@project.price_upto) %>
                                        </p>
                                        <p>
                                            <span>Possession Date</span>
                                            <%= @project.possession.present? ? @project.possession.strftime('%d %b, %Y') :  '-' %>
                                        </p>
                                    </div>
                                    <div class="col-lg-7 col-12 ps-3 overview-desc float-end">
                                        <div class="project-overview-desc"><%= @project.description&.html_safe %></div>
                                    </div>
                                </div>
                            </div>
                            <div class="prjt-desc-tab bt-br col-12 pt-4 pb-4 float-start" id="tab-location">
                                <div class="tab-header col-12 float-start">
                                    <h2 class="tab-title float-start">Location</h2>
                                    <!--<a href="javascript:;" class="location-share float-end"><i class="fa fa-share-alt fill-primary me-2"></i>Share location</a>-->
                                </div>
                                <div class="tab-body col-12 float-start pt-3">
                                    <div class="col-lg-6 col-12 float-start">
                                        <div class="location-list">
                                          <p><i class="fa fa-map-marker-alt me-2"></i><%= "#{@project.micro_market}, #{@project.city}" %></p>
                                          <% if @project.nearby_locations.present? %>
                                            <ul>
                                              <% @project.nearby_locations.each do |nb_loc| %>
                                                <li>
                                                  <p><span><%= nb_loc.distance %> <%= nb_loc.unit.pluralize(nb_loc.distance.to_i)&.titleize %></span> <%= nb_loc.destination&.titleize %></p>
                                                </li>
                                              <% end %>
                                            </ul>
                                          <% end %>
                                        </div>
                                    </div>
                                    <% if @project.embed_map_tag.present? %>
                                      <div class="col-lg-6 col-12 float-end embed-map">
                                        <%= @project.embed_map_tag.html_safe %>
                                      </div>
                                    <% end %>
                                </div>
                            </div>
                            <% if inventory_avl %>
                            <div class="prjt-desc-tab col-12 pt-4 float-start" id="tab-configure">
                                <div class="tab-header col-12 float-start position-relative">
                                    <h2 class="tab-title float-start">Configurations</h2>
                                    <a href="javascript:;" class="emi-calc-btn"><p class="float-end">EMI's starting at <strong>₹ 25,000</strong> /month</p>                                   
                                    </a>
                                     <div class="card emi-wrapper position-absolute top-50 end-0">
                                        <div class="card-header bg-primary py-4  d-flex justify-content-between">
                                            <h2 class="fn-20 white mb-0">EMI</h2>
                                            <p class="fn-20 font-medium white mb-0">₹ <span class="emiValue"></span></p>
                                            <a href="javascript:;" class="close-emi position-absolute top-0 end-0 me-2 mt-1"><i class="fa far fa-times fill-white"></i></a>
                                        </div>
                                        <div class="card-body">
                                            <div id="widget">
                                                <div class="wid_left">
                                                    <div class="form-group">
                                                        <div class="range_indicator">
                                                            <label for="loan-amount">Home Loan Amount <span>In Lakhs</span> </label>
                                                            <div class="indi_rgt">₹  <span id="selected-amount"></span> Lakhs </div>
                                                        </div>
                                                        <div id="loan-amount"></div>
                                                        <span class="scalerange">
                                                            <span>0</span>
                                                            <span>25</span>
                                                            <span>50</span>
                                                            <span>75</span>
                                                            <span>100</span>
                                                            <span>125</span>
                                                            <span>150</span>
                                                            <span>175</span>
                                                            <span>200</span>
                                                        </span>
                                                    </div>

                                                    <div class="form-group">
                                                        <div class="range_indicator">
                                                            <label for="loan-duration">Rate of Interest</label>
                                                            <div class="indi_rgt"><span id="selected-interest"></span> % </div>
                                                        </div>
                                                        <div id="interest-rate"></div>
                                                        <span class="scalerange">
                                                            <span>5</span>
                                                            <span>7.5</span>
                                                            <span>10</span>
                                                            <span>12.5</span>
                                                            <span>15</span>
                                                            <span>17.5</span>
                                                            <span>20</span>
                                                            <span>22.5</span>
                                                        </span>
                                                    </div>

                                                    <div class="form-group">
                                                        <div class="range_indicator">
                                                            <label for="loan-duration">Loan Tenure</label>
                                                            <div class="indi_rgt"><span id="selected-duration"></span> Years </div>
                                                        </div>
                                                        <div id="loan-duration"></div>
                                                        <span class="scalerange">
                                                            <span>1</span>
                                                            <span>5</span>
                                                            <span>10</span>
                                                            <span>15</span>
                                                            <span>20</span>
                                                            <span>25</span>
                                                            <span>30</span>
                                                            <span>35</span>
                                                        </span>
                                                    </div>

                                                    <div class="wid_right list-unstyled d-flex justify-content-between">
                                                        <div class="fn-14">Total Interest Payable <p class=" fn-20 font-medium">₹ <span class="interestValue"></span></p>
                                                        </div>
                                                        <div class="fn-14">Total Amount Payable 
                                                            <!-- <p class="fn-14 mb-0">Principal + Interest</p> -->
                                                            <p class="fn-20 font-medium">₹ <span class="TotalValue "></span></p>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab-body col-12 float-start">
                                    <div class="col-12">
                                        <div class="prjt-configure-list">
                                            <h2>Select Type</h2>
                                            <div class="tower-type tab-link nav nav-tabs border-bottom-0" role="tablist">
                                              <% @project.unit_configurations.each_with_index do | uc, index | %>
                                                <a href="javascript:;" class="nav-item nav-link mb-1 <%= (index == 0) ? "active" : "" %>" data-bs-toggle="tab" data-bs-target="#floor_<%= uc.name.gsub(/[()\s]*/, '').underscore + uc.saleable.to_i.to_s %>" role="tab"><%= uc.name %> | (<%= uc.saleable %> sq.ft)</a>
                                            <% end %>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-12 float-start configration-tab">
                                        <div class="tab-content br-1 br-rd-8 py-1 wd-100">
                                        <% @project.unit_configurations.each_with_index do | uc, index | %>
                                          <% units = uc.project_units %>
                                          <% if units.present? %>
                                            <% unit = units&.first %>
                                            <div class="tab-pane fade show <%= (index == 0) ? "active" : "" %>" id="floor_<%= uc.name&.gsub(/[()\s]*/, '').underscore + uc.saleable.to_i.to_s %>" role="tabpanel">
                                                <div class="col-5 float-start">
                                                  <% if url = uc.assets.first&.file&.url&.presence %>
                                                    <a href="javascript:;" data-bs-toggle="modal" data-bs-target="#configurationModal">
                                                        <img src="<%= url %>" class="plan-img img-fluid">
                                                    </a>
                                                  <% end %>
                                                    <!--<p class="ps-2"><i class="fa fa-eye me-2"></i>220 Interests</p>-->
                                                </div>
                                                <div class="col-7 float-end">
                                                    <div class="prjt-config-info mt-3 pe-3">
                                                        <div class="left-info col-12 float-start pb-2">
                                                            <span><i class="fa fa-building"></i></span>
                                                            <div class="prjt-info">
                                                                <p><%= unit.project_tower_name %></p>
                                                                <span><%= units.collect(&:floor).max %> Floors | <%= units.map(&:floor_order).uniq.count %> Apartments per floor</span>
                                                            </div>
                                                        </div>
                                                        <div class="col-6 float-start">
                                                            <p class="prjt-short-info"><%= uc.name %> (<%= uc.saleable %> sq.ft) <%= number_to_inr(unit.all_inclusive_price) %></p>
                                                        </div>
                                                        <% avl_unit = units.where(status: 'available').first.presence %>
                                                        <% if avl_unit %>
                                                          <div class="col-6 float-end">
                                                              <%= link_to 'View Cost Sheet', quotation_admin_project_unit_path(avl_unit, layout: false), class: 'cost-sheet-btn float-end modal-remote-form-link', style: 'height: auto' %>
                                                          </div>
                                                        <% end %>
                                                        <% if @project.usp.present? %>
                                                          <div class="col-12 bt-bt mt-3 float-start pt-2">
                                                              <div class="col-6 float-start">
                                                                  <p class="points-list-title">Uniques Selling Points</p>
                                                              </div>
                                                              <div class="col-6 float-end">
                                                                  <a href="javascript:;" class="float-end view-all-btn">View All</a>
                                                              </div>
                                                              <ul class="points-list col-12 float-start">
                                                                <% @project.usp.first(3).each do |usp| %>
                                                                  <li>
                                                                    <p><%= usp %></p>
                                                                  </li>
                                                                <% end %>
                                                              </ul>
                                                          </div>
                                                        <% end %>
                                                    </div>
                                                </div>
                                            </div>
                                          <% end %>
                                        <% end %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% end %>
                            <div class="prjt-desc-tab col-12 bt-bt float-start" id="tab-collaterals">
                                <div class="tab-header col-12 float-start">
                                    <h2 class="tab-title float-start">Collaterals</h2>
                                    <!-- <a href="javascript:;" class="location-share float-end mt-2"><i class="fa fa-share-alt fill-primary me-2"></i>Share collaterals</a>-->
                                </div>
                                <div class="tab-body">
                                    <div class="col-12 mb-3 float-start">
                                        <div class="prjt-gallery">
                                            <h2>Gallery</h2>
                                            <div class="prjt-gallery-inner">
                                                <ul>
                                                    <% @assets = @project.assets.where(document_type: "images") %>
                                                    <% @assets[0..5].each_with_index do |asset, index|%>
                                                        <li class="<%= (@assets.size > 5 && index == 5) ? "more-img" : "" %>">
                                                            <a href="javascript:;" data-bs-toggle="modal" data-bs-target="#galleryModal">
                                                                <div class="wd-100 ht-77 rounded" style="background-image: url(<%= asset.file.url %>);background-size:cover;background-position:center"></div>
                                                                <% if (@assets.size > 5 && index == 5) %>
                                                                <p>+ <%= @assets.size - 6 %></p>
                                                                <% end %> 
                                                            </a>
                                                        </li>
                                                    <% end %>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    
                                    <div class="col-12 mb-3 float-start">
                                        <div class="prjt-gallery">
                                            <h2>Video</h2>
                                            <ul class="collaterals-list">
                                              <% @project.videos.each do |video| %>
                                                <li>
                                                    <div class="prjt-video">
                                                      <%= image_tag video.thumbnail&.url, class: 'img-fluid' %>
                                                      <%= link_to '<i class="fa fa-play-circle"></i>'.html_safe, videoable_path(videoable_type: @project.class.model_name.i18n_key.to_s, videoable_id: @project.id, id: video.id), class: 'modal-remote-form-link' %>
                                                      <div class="video-info">
                                                        <span class='text-truncate'><%= video.description %></span>
                                                      </div>
                                                    </div>
                                                </li>
                                              <% end %>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col-12 mb-3 float-start">
                                        <div class="prjt-gallery">
                                            <h2>Documents</h2>
                                            <ul class="documents-list">
                                                <% @assets = @project.assets.where(document_type: "document") %>
                                                    <% @assets.each_with_index do |asset, index|%>
                                                    <%
                                                    mime_type = asset.file.content_type 
                                                    case mime_type
                                                    when "application/pdf"
                                                    filetype = "pdf"
                                                    when "application/doc"
                                                    filetype = "docs"
                                                    when /^excel/
                                                    filetype = "excel"
                                                    when /^ppt/
                                                    filetype = "ppt"
                                                    else
                                                    filetype = "pdf"
                                                    end
                                                    %>
                                                        <li>
                                                            <div class="docs-wrap">
                                                                <div class="docs-icon">
                                                                   <a href="<%= asset.file.url %>" target="_blank"> <img src='<%= asset_path "#{filetype}-icon.png" %>' class="img-fluid"></a>
                                                                </div>
                                                                <div class="docs-info">
                                                                    <span><%= asset.file_name %></span>
                                                                    <span class="video-download">
                                                                    <a href="<%= asset.file.url %>" target="_blank">
                                                                        <i class="fa fa-download"></i>
                                                                        </a>
                                                                    </span>
                                                                    </a>
                                                                </div>
                                                            </div>
                                                        </li>
                                                    <% end %>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% if @project.amenities.present? %>
                            <div class="prjt-desc-tab col-12 bt-bt float-start" id="tab-amenities">
                                <div class="tab-header col-12 float-start">
                                    <h2 class="tab-title float-start">Amenities</h2>
                                    <!--<p class="drop-amenities float-end">
                                        <span>Now Showing:</span>
                                        <select class="form-control d-inline me-1">
                                            <option>All Amenities</option>
                                            <option>Amenities</option>
                                            <option>Amenities</option>
                                        </select>
                                    </p>-->
                                </div>
                                <div class="tab-body">
                                    <div class="col-12">
                                        <div class="amenities-list">
                                            <ul class="">
                                                <% @project.amenities.each do |type| %>
                                                    <li class="amenity_<%= type %>">
                                                    <div class="amenities-wrap">
                                                        <span class="text-center d-block">
                                                          <i class="fad <%= t("amenity_icons.#{type}") %> fn-20 mx-auto"></i>
                                                        </span>
                                                    <p><%= type.titleize %></p>
                                                    </div>
                                                    </li>
                                                <% end %>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% end %>
                            <% if @project.specifications.present? %>
                            <div class="prjt-desc-tab col-12 bt-bt float-start" id="tab-specifications">
                                <div class="tab-header col-12 float-start">
                                    <h2 class="tab-title">Specifications</h2>
                                </div>
                                <div class="tab-body">
                                    <div class="col-12">
                                        <div class="specifications-tab-wrap">
                                            <div class="row mt-4 specification-list col-12">
                                            <% @project.specifications.each do |spec| %>
                                                <div class="col-lg-6 mb-3">
                                                <h3><%= spec.category.titleize %></h3>
                                                <div><%= spec.description&.html_safe %></div>
                                                </div>
                                              <% end %>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% end %>
                            <% if @project.timeline_updates.present? %>
                            <div class="prjt-desc-tab col-12 bt-bt float-start" id="tab-timeline">
                                <div class="tab-header col-12 float-start pb-3">
                                    <h2 class="tab-title">Project Timeline</h2>
                                </div>
                                <div class="tab-body pb-5">
                                  <% if @project.timeline_updates.present? %>
                                    <div class="col-12 float-start pb-2">
                                        <div class="prjt-current-status position-relative pb-5 pb-xs-0">
                                            <div class="line">
                                                <span class="inner-line"></span>
                                            </div>
                                            <div class="current-status-tooltip">
                                              <p>Current Status: <%= @project.category&.first %></p>
