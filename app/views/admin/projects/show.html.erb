                                              <div class="prjt-<%= index.zero? ? 'start' : (index == tu_count-1 ? 'end' : 'status')%>">
                                                <p>
                                                <span><%= tu.date.strftime('%b %Y') %></span>
                                                <span><%= tu.name %></span>
                                                </p>
                                              </div>
                                            <% end %>
                                        </div>
                                    </div>
                                  <% end %>
                                </div>
                              </div>
