<table class='table my-customer-table responsive-tbl'>
  <thead class="th-default">
    <thead>
      <tr class="bg-primary white">
        <th><%= t('mongoid.attributes.push_notification.title') %></th>
        <th><%= t('mongoid.attributes.push_notification.content') %></th>
        <th><%= t('mongoid.attributes.push_notification.data') %></th>
        <th><%= t('mongoid.attributes.push_notification.status') %></th>
      </tr>
    </thead>
  </thead>
  <tbody>
    <% push_notifications.each do |push_notification| %>
      <tr>
        <td data-title=<%= t('mongoid.attributes.push_notification.title') %> >
          <%=  (push_notification.title || "-") %>
        </td>
        <td data-title=<%= t('mongoid.attributes.push_notification.content') %> >
          <%= (push_notification.content || "-") %>
        </td>
        <td data-title=<%= t('mongoid.attributes.push_notification.data') %> >
          <%= (push_notification.data || "-") %>
        </td>
        <td data-title=<%= t('mongoid.attributes.push_notification.status') %> >
          <%= (push_notification.status || "-") %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<div class="col-lg-12 py-3 white mb-2 px-2">
  <div class="row">
    <div class="col-lg-6">
      <p class="table-entry mb-0 ps-2 pt-2"><%= page_entries_info push_notifications %></p>
    </div>
    <div class="col-lg-6 table-pagination">
      <%= will_paginate push_notifications, list_classes: %w(pagination justify-content-end) %>
    </div>
  </div>
</div>

