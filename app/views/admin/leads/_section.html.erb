<% hex_id = "collapseLeads#{SecureRandom.hex(4)}" %>
<section class="leads mb-4">
  <div class="">
    <div class="row">
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12" id="<%= hex_id %>">
        <% if !current_user.buyer? %>
          <%= render "admin/leads/index", leads: leads %>
        <% end %>
      </div>
    </div>
  </div>
</section>
<%= javascript_tag do %>
  $(document).ready(function(){
      var count = <%= raw leads.count %>
      if(count != 0){
        $('.collapseLeads').collapse('show')
      }
  });
<% end %>
