<section class="user-db-sec-6 device-pd">
  <div class="box-card">
    <a data-bs-toggle="collapse" href="#project_wise_user_requests_report" role="button" aria-expanded="false" aria-controls="project_wise_user_requests_report" class='font-weight-bold text-decoration-none'>
      <div class="card-header text-start text-white bg-theme">
        <%= I18n.t("controller.dashboard.link_to.user_requests.details_report") %>
      </div>
    </a>
    <div class="card-body collapse p-0" id='project_wise_user_requests_report'>
      <div class="row" id = "project_wise_user_requests_report"></div>
    </div>
  </div>
</section>

<%= javascript_tag do %>
  $(document).ready(function(){
    FormInitializer.init($("#project_wise_user_requests_report_filter"));
    $.ajax({
      url: "<%= project_wise_user_requests_report_path %>",
      type: "get",
    });
  });
<% end %>
