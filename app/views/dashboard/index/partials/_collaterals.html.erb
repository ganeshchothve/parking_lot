<section class="user-db-sec-5 device-pd">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 no-pd">
        <div class="row">
          <div class="box-card col-lg-12 col-md-12 col-sm-12 col-xs-12 mt-5 collaterals">
            <div class="box-header bg-gradient-cd br-rd-tr-4">
              <h2><img src="<%= asset_path 'folder-white.svg' %>" alt="My Documents" class="wd-18">Collaterals</h2>
              <%= link_to("View All", collaterals_admin_projects_path, class: 'view-all-btn modal-remote-form-link') %>
            </div>
            <div class="box-content br-rd-bl-4 bg-white">
              <div class="pd-box">
                <div class="list-group">
                  <% Project.all.each do |project| %>
                    <%= link_to project.name, collaterals_admin_project_path(project), class: 'list-group-item list-group-item-action modal-remote-form-link' %>
                  <% end %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
