<div class="container">
  <div class  = "card">
    <div class = "row m-0">
      <div class = 'col-2 border' align='center'>
        Customers</br><%= DashboardData::AdminDataProvider.customer_count %>
      </div>
      <div class = 'col-2 border' align='center'>
        Channel Partners</br><%= DashboardData::AdminDataProvider.channel_partner_count %>
      </div>
      <div class = 'col-2 border' align='center'>
        Units </br><%= DashboardData::AdminDataProvider.sold_project_units_count %> sold /<%= DashboardData::AdminDataProvider.all_project_units_count %>
      </div>
      <div class = 'col-2 border' align='center'>
        User Requests </br><%= DashboardData::AdminDataProvider.open_user_requests %>
      </div>
      <div class = 'col-2 border' align='center'>
        Payments </br><%= DashboardData::AdminDataProvider.online_receipts %> online/<%= DashboardData::AdminDataProvider.offline_receipts %> offline
      </div>
      <div class = 'col-2 border' align='center'>
        Empty for now
      </div>
    </div>
  </div>
  <div class = "row pt-3">
    <div class = "col-6">
      <div id = "dashboard_data_barchart">
        <p> Loading .. </p>
      </div>
    </div>
    <div class = "col-6 ">
      <div id = "dashboard_data_linechart">
        <p> Loading .. </p>
      </div>
    </div>
  </div>
  <div class = "row pt-3">
    <div class = "col-6">
      <div id="project_unit_barchart">
        <p> Loading .. </p>
      </div>
    </div>
    <div class = "col-6">
      <div id="booking_detail_barchart">
        <p> Loading .. </p>
      </div>
    </div>
  </div>
  <div class = "row pt-3">
    <div class = "col-6">
      <div id = "dashboard_data_piechart">
        <p> Loading .. </p>
      </div>
    </div>
    <div class = "col-6">
      <div id = "user_dashboard_barchart">
        <p> Loading .. </p>
      </div>
    </div>
  </div>
</div>
  <div class="mt-3"></div>
  <%= render "dashboard/index/partials/users" %>
  <div class="mt-3"></div>
  <%= render "dashboard/index/partials/receipts" %>
  <div class="mt-3"></div>
  <%= render "dashboard/index/partials/project_units" %>
  <div class="mt-3"></div>
  <%= render "dashboard/index/partials/user_requests" %>
  <div class="pb-5"></div>
</div>
<%= javascript_tag do %>
  $(document).ready(function(){
    $.ajax({
      url: "<%= dashboard_data_barchart_admin_receipts_path %>",
      type: "get",
    });
    $.ajax({
      url: "<%= dashboard_data_piechart_admin_receipts_path %>",
      type: "get",
    });
    $.ajax({
      url: "<%= dashboard_data_linechart_admin_receipts_path %>",
      type: "get",
    });
    $.ajax({
      url: "<%= project_unit_barchart_admin_project_units_path %>",
      type: "get",
    });
    $.ajax({
      url: "<%= booking_detail_barchart_admin_booking_details_path %>",
      type: "get",
    });
    $.ajax({
      url: "<%= user_dashboard_barchart_admin_users_path %>",
      type: "get",
    });
  });
<% end %>