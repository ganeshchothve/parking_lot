          <g transform="matrix(1, 0, 0, 1, 184, 2508)" filter="url(#Rectangle_3440)">
            <rect id="Rectangle_3440-3" data-name="Rectangle 3440" width="1001" height="14" rx="7" transform="translate(7 6)" fill="#fff"/>
          </g>
        </g>
      </g>
    </svg>              
    <img src="<%= asset_path 'leaderboard/rocket.png' %>" class="rocket-img">
    <img src="<%= asset_path 'leaderboard/rocket-power.svg' %>" class="rocket-power-img">
    <p class="target-value"><%= (@achieved_target_data.first[:achieved_target] rescue "-") %></p>
  </div>
</div>
