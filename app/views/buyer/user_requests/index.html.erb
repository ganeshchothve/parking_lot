<section class="mb-5">
  <div class="container">
    <div class="row">
    <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
      <div class="table-title pt-1 pb-3">
        <h1 class="title icon-set icon-file-contract-black">
          <%= UserRequest.model_name.human(count: @user_requests.total_entries ) %>
          <span class="badge bg-gradient-cd white"><%= @user_requests.total_entries %></span>
        </h1>
      </div>
    </div>
    <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 no-pd">
      <div class="table-btn-set float-right pb-3">
      </div>
    </div>
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 no-pd">
        <div class="box-card relative">
          <%= render 'admin/user_requests/index', {user_requests: @user_requests} %>
        </div>
      </div>
    </div>
  </div>
</section>
