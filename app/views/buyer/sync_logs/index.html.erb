      <div class="card">
        <div class="card-body">
          <div class="row mb-3">
            <div class="col-md-7"></div>
            <div class="col-md-5 text-md-right">
              <h6 class="mt-2">Total Synclogs : <%= @sync_logs.total_entries %></h6>
            </div>
          </div>
          <%= render 'admin/sync_logs/index', {sync_logs: @sync_logs} %>
        </div>
      </div>
    </div>
  </div>
</div>
