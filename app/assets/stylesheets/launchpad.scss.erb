.webinar-sector-image {
    position: absolute;
    top: -3%;
    left: 6%;
    z-index: -1;
    /*-webkit-animation:sectorrotation 2s linear;*/
    animation: sectorrotation 10s linear alternate;
    transform-origin: bottom right;
}

@keyframes sectorrotation {

    to {
        transform: rotate(180deg);
    }
}

