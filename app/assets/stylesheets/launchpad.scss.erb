.incentive-sec{
    background-color:#fff;
    h2{
        margin-bottom:50px;
        color:#078181;
    }
    .bx-wrapper{
        max-width: 100%!important;
        img{
            width: 100%;
        }
    }
    .tab-box{
        position: relative;
        &:after{
            content: '';
            position: absolute;
            background: transparent linear-gradient(180deg, #00000000 0%, #000000 100%) 0% 0% no-repeat padding-box;
            opacity: 0.77;
            width:100%;
            height:90%;
            bottom:0px;
            left:0px;
            @media screen and (max-width:700px){
                opacity: 0.97;
            }
        }
        .inner-box{
            position: absolute;
            bottom: 0px;
            z-index: 2;
            color: #fff;
            width: 100%;
            bottom: 50px;
            padding: 0 50px;
            height:200px;
            transition: all 0.6s ease-in-out;
            h3{
                font-size: 24px;
                line-height:33px;
                margin-bottom:25px;
                @media screen and (max-width:700px){
                    margin-bottom: 15px;
                    font-size: 22px;
                    font-weight: 600;
                }
            }
            ul{
                margin-left: 20px;
                
                li{
                    list-style: disc;
                    line-height:22px;
                    margin-bottom:10px;
                }
            }  
            @media screen and (max-width:700px){
                padding: 0 20px;
                bottom:10px;
                height:auto;
            }          
        }
        /* &:hover{
            .inner-box{
                bottom:100px;
                height:150px;
            }
        } */
    }
    .incentive-lists{
        li{
            padding-bottom:100px;
            position: relative;
            &:before {
                content: '';
                position: absolute;
                left: 7px;
                top: 8px;
                border: 1px dashed #07B6B6;
                height: 100%;
                width: 1px;
            }
            &:last-child:before{
                display:none;
            }
            a{
                color:#fff;
                font-size:18px;
                line-height:24px;
                font-weight:500;
            }
        }
    }
}
.incentive-sec {
    .incentive-lists{
        li{
            a{
                color: #fff;
                font-size: 18px;
                line-height: 24px;
                font-weight: 500;
                position: relative;
                text-align: left;
                padding-left: 25px;
                display: block;
                &:before{
                    content: '';
                    position: absolute;
                    width: 9px;
                    height: 9px;
                    border-radius: 100%;
                    left: 3px;
                    top: 7px;
                    background-color: #07B6B6;
                }
            }
            &.active{
                a{
                    &:before{
                        content: '';
                        position: absolute;
                        width: 9px;
                        height: 9px;
                        border-radius: 100%;
                        left: 3px;
                        top: 7px;
                        background-color: #fff;
                    }
                    &:after{
                        content: '';
                        position: absolute;
                        width: 15px;
                        height: 15px;
                        border-radius: 100%;
                        border: 1px solid #fff;
                        left: 0;
                        top: 4px;
                    }
                }
            }
        }
    }
    .tab-content{
        display:none;
        &.active{
            display:block;
        }
    }
}
