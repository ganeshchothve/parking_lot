.broker-sec {
    position: relative;
    background: #E7F5FC 0% 0% no-repeat padding-box;
    &:before {
        content: '';
        position: absolute;
        width: 50%;
        height: 100%;
        left: 0;
        top: 0;
        background: transparent linear-gradient(180deg, #CFEAF7 0%, #CFEAF700 100%) 0% 0% no-repeat padding-box;
        z-index: 0;
    }
    .container{
        position: relative;
        z-index: 2;
        width:95%;
        max-width: 95%;
    }
    .broker-wrapper{
        height:600px;
        position: relative;
        margin-bottom:50px;
        .broker-img{
            position: absolute;
            bottom:0;
            left:50%;
            transform:translateX(-50%);
        }
        .broker-left, .broker-right{
            width:400px;
            h2{
                letter-spacing: 0px;
                color: #151515;
                text-transform: uppercase;
                font: normal normal 800 42px/58px Manrope;
                margin-bottom:100px;
            }
            ul{
                width:400px;
                li{
                    border-radius:6px;
                    height:58px;
                    padding: 15px 25px;
                    font-size:16px;
                    line-height:22px;
                    font-weight:700;
                    font-family: 'Manrope', sans-serif;
                    background-color:#fff;
                    display: inline-block;
                    margin-bottom: 10px;
                }
            }
            &.broker-left{
                position: absolute;
                bottom:20px;
                ul{
                    li{
                        color:#024948;
                    }
                }
            }
            &.broker-right{
                position: absolute;
                bottom:20px;
                right:0px;
                ul{
                    li{
                        color:#07B6B6;
                    }
                }
            }
        }
        @media screen and (max-width: 700px){
            height: auto;
            margin-top: 30px;
            .broker-left, .broker-right{
                position: unset!important;
                width: auto!important;
                ul{
                    width: auto!important;
                    li{
                        height: auto;
                        padding: 8px 15px;
                        font-size: 14px;
                    }
                }
                h2{
                    font-size: 18px;
                    line-height: 26px;
                    margin-bottom: 20px;
                    margin-top: 20px;
                }
            }
            .broker-left{
                text-align: left!important;
            }
            .broker-right{
                text-align: right!important;
                ul{
                    text-align: right!important;
                }
            }
            .broker-img{
                position: unset;
                transform: unset;
                img{
                    width: 100%;
                    margin-top:-40px;
                }
            }
        }
    }
}
