    letter-spacing: 2px;
}

.new-line-2 {
    position: relative;
    font-size: 37.45px;
    line-height: 53.06px;
    letter-spacing: 1.5px;
}

.new-line-2:before {
    content: '';
    position: absolute;
    left: 0px;
    top: 39%;
    width: 40%;
    height: 8px;
    border-top: 1px solid #fff;
    border-bottom: 1px solid #fff;
}

.new-line-2:after {
    content: '';
    position: absolute;
    right: 0px;
    top: 39%;
    width: 40%;
    height: 8px;
    border-top: 1px solid #fff;
    border-bottom: 1px solid #fff;
}

.new-line-3 {
    font-size: 52px;
    line-height: 85px;
    letter-spacing: 2px;
}

.new-line-4 {
    font-size: 63.36px;
    line-height: 54.44px;
    letter-spacing: 2.53px;
    position: relative;
    padding-bottom: 20px;
    color: #0EB8B7;
}

.new-line-4:before {
    content: '';
    position: absolute;
    bottom: 0px;
    left: 0px;
    border-bottom: 1px solid #fff;
    width: 100%;
}

.new-line-4:after {
    content: '';
    position: absolute;
    width: 15px;
    height: 15px;
    background-color: #fff;
    margin: auto;
    bottom: -8px;
    left: 0px;
    right: 0px;
    transform: rotate(45deg);
}

